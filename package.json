		"test:staged": "npm test -- --findRelatedTests --watchAll=false"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/plinioduartt/swagger-node.git"
	},
	"keywords": [
		"swagger",
		"node",
		"typescript"
	],
	"author": "plinioduartt",
