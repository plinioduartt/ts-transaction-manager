{
	"name": "swagger-node",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "rimraf ./dist && tsc -p ./tsconfig.json",
		"dev": "tsc --watch",
		"lint": "eslint --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"test": "npx jest --passWithNoTests  --runInBand --detectOpenHandles",
