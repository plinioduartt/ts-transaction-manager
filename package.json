	"bugs": {
		"url": "https://github.com/plinioduartt/swagger-node/issues"
	},
	"homepage": "https://github.com/plinioduartt/swagger-node#readme",
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"@types/node": "^18.14.0",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"eslint": "^7.32.0",
		"eslint-config-standard-with-typescript": "^21.0.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.2.0",
		"git-commit-msg-linter": "^3.2.8",
		"husky": "^4.3.8",
		"jest": "^28.1.3",
		"lint-staged": "^12.4.1",
		"prettier": "^2.8.4",
		"ts-jest": "^28.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"rimraf": "3.0.2"
	},
	"dependencies": {
		"reflect-metadata": "^0.1.13"
	}
